<!DOCTYPE html>
<html>
<head>
	<title>NewStore</title>
	<%= stylesheet_link_tag    "application", :media => "all" %>
	<%= javascript_include_tag "application" %>
	<%= csrf_meta_tags %>
</head>
<body>
	<div class="notice">
		<%= flash[:notice] %>
	</div>
	<div class="errors">
		<%= flash[:errors] %>
	</div>
	<div class="container">
		<%= render 'menu/top_bar' %>
		<div class="row">
			<%= render 'menu/right' %>
			<div class="span7">
				<%= yield %>
			</div>
			<div class="big_product_image span3" style='background: red; height: 100%'>
				
			</div>
		</div>
	</div>
	

</body>
</html>

